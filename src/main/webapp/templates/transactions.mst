<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>&nbsp;</th>
      <th>Description</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody>
	{{#transactions}}
	<tr>
	    <td>{{date}}</td>
	    <td>{{category}}</td>
	    <td>{{name}}</td>
	    <td class=".text-right currency">{{amount}}</td>
	</tr>
	{{/transactions}}
  </tbody>
</table>